    .container-scroller
      // partial:partials/_navbar.html
      nav.navbar.col-lg-12.col-12.p-0.fixed-top.d-flex.flex-row
        .text-center.navbar-brand-wrapper.d-flex.align-items-center.justify-content-center
          a.navbar-brand.brand-logo.mr-5(href='index.html')
            img.mr-2(src='images/gametime-logo.png' alt='logo')
          a.navbar-brand.brand-logo-mini(href='index.html')
            img.mr-2(src='images/gametime-logo.png' alt='logo')
        .navbar-menu-wrapper.d-flex.align-items-center.justify-content-end
          button.navbar-toggler.navbar-toggler.align-self-center(type='button' data-toggle='minimize')
            span.icon-menu
          ul.navbar-nav.mr-lg-2
            li.nav-item.nav-search.d-none.d-lg-block
              .input-group
                #navbar-search-icon.input-group-prepend.hover-cursor
                  span#search.input-group-text
                    i.icon-search
                input#navbar-search-input.form-control(type='text' placeholder='Search now' aria-label='search' aria-describedby='search')
          ul.navbar-nav.navbar-nav-right
            li.nav-item.dropdown
              a#notificationDropdown.nav-link.count-indicator.dropdown-toggle(href='#' data-toggle='dropdown')
                i.icon-bell.mx-0
                span.count
              .dropdown-menu.dropdown-menu-right.navbar-dropdown.preview-list(aria-labelledby='notificationDropdown')
                p.mb-0.font-weight-normal.float-left.dropdown-header Notifications
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    .preview-icon.bg-success
                      i.ti-info-alt.mx-0
                  .preview-item-content
                    h6.preview-subject.font-weight-normal Application Error
                    p.font-weight-light.small-text.mb-0.text-muted
                      | Just now
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    .preview-icon.bg-warning
                      i.ti-settings.mx-0
                  .preview-item-content
                    h6.preview-subject.font-weight-normal Settings
                    p.font-weight-light.small-text.mb-0.text-muted
                      | Private message
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    .preview-icon.bg-info
                      i.ti-user.mx-0
                  .preview-item-content
                    h6.preview-subject.font-weight-normal New user registration
                    p.font-weight-light.small-text.mb-0.text-muted
                      | 2 days ago
            li.nav-item.nav-profile.dropdown
              a#profileDropdown.nav-link.dropdown-toggle(href='#' data-toggle='dropdown')
                img(src='images/faces/face28.jpg' alt='profile')
              .dropdown-menu.dropdown-menu-right.navbar-dropdown(aria-labelledby='profileDropdown')
                a.dropdown-item
                  i.ti-settings.text-primary
                  |                 Settings
                a.dropdown-item
                  i.ti-power-off.text-primary
                  |                 Logout
            li.nav-item.nav-settings.d-none.d-lg-flex
              a.nav-link(href='#')
                i.icon-ellipsis
          button.navbar-toggler.navbar-toggler-right.d-lg-none.align-self-center(type='button' data-toggle='offcanvas')
            span.icon-menu
